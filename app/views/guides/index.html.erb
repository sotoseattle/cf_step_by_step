<h1>Guides</h1>
<ul>
  <% @guides.each do |g| %>
  <li>
    <span><%= g.name %></span>
    <span><%= g.description %></span>
    <span><%= link_to "Show", g %> </span>
    <span><%= link_to "Edit", edit_guide_path(g) %> </span>
    <span><%= link_to "Delete", g, method:'delete' %> </span>
  </li>
  <% end %>
</ul>

<%= link_to "Create New Guide", new_guide_path %>
